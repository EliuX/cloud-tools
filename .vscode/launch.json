        "!**/node_modules/**"
      ],
      "cwd": "${workspaceFolder}/azure/storage/storage-migrator"
    },
    {
      "name": "Debug: Storage Validate Credentials",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/storage/storage-migrator/cli.js",
      "args": [
        "validate"
      ],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/azure/storage/storage-migrator/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/azure/storage/storage-migrator/**",
        "!**/node_modules/**"
      ],
      "cwd": "${workspaceFolder}/azure/storage/storage-migrator"
    },
    {
      "name": "Debug: Storage Custom Arguments",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/storage/storage-migrator/cli.js",
      "args": [],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/azure/storage/storage-migrator/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/azure/storage/storage-migrator/**",
        "!**/node_modules/**"
      ],
      "cwd": "${workspaceFolder}/azure/storage/storage-migrator"
    },
    {
      "name": "Debug: Storage Sync Queues",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/storage/storage-migrator/cli.js",
      "args": [
        "sync-queues",
        "--preserve-destination-queues"
      ],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/azure/storage/storage-migrator/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/azure/storage/storage-migrator/**",
        "!**/node_modules/**"
      ],
      "cwd": "${workspaceFolder}/azure/storage/storage-migrator"
    },
    {
      "name": "Debug: Storage Compare Accounts",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/storage/storage-migrator/cli.js",
      "args": [
        "compare",
        "--source-connection-string", "DefaultEndpointsProtocol=https;AccountName=sourceaccount;AccountKey=sourcekey;EndpointSuffix=core.windows.net",
        "--destination-connection-string", "DefaultEndpointsProtocol=https;AccountName=destaccount;AccountKey=destkey;EndpointSuffix=core.windows.net",
        "--include-services", "blobs,queues",
        "--output-file", "comparison-report.json"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/azure/storage/storage-migrator"
    },
    {
      "name": "Debug: Storage Sync Containers",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/storage/storage-migrator/cli.js",
      "args": [
        "sync-containers",
        "--source-connection-string", "DefaultEndpointsProtocol=https;AccountName=sourceaccount;AccountKey=sourcekey;EndpointSuffix=core.windows.net",
        "--destination-connection-string", "DefaultEndpointsProtocol=https;AccountName=destaccount;AccountKey=destkey;EndpointSuffix=core.windows.net",
        "--preserve-destination-containers",
        "--preserve-metadata"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/azure/storage/storage-migrator"
    }
  ]
