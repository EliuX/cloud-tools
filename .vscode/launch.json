{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: Cosmos Migration (Full)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/cli.js",
      "args": [
        "migrate",
        "--overwrite"
      ],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/**",
        "!**/node_modules/**"
      ],
      "cwd": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator"
    },
    {
      "name": "Debug: Cosmos Extract Schema",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/cli.js",
      "args": [
        "extract",
        "--output", "debug-schema.json"
      ],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/**",
        "!**/node_modules/**"
      ],
      "cwd": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator"
    },
    {
      "name": "Debug: Cosmos Create Schema",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/cli.js",
      "args": [
        "create",
        "debug-schema.json",
        "--overwrite"
      ],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/**",
        "!**/node_modules/**"
      ],
      "cwd": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator"
    },
    {
      "name": "Debug: Cosmos List Databases",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/cli.js",
      "args": [
        "list"
      ],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/**",
        "!**/node_modules/**"
      ],
      "cwd": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator"
    },
    {
      "name": "Debug: Cosmos Migration with Data",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/cli.js",
      "args": [
        "migrate",
        "--include-data",
        "--overwrite"
      ],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/azure/cosmosdb/cosmos-migrator/.env",
